{
  "name": "ngx-timeago-demo",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng:build:demo": "ng build packages --prod --base-href=https://ihym.github.io/ngx-timeago/",
    "build:demo": "cpr dist/ngx-timeago demo/ngx-timeago && npm run ng:build:demo && rimraf demo/ngx-timeago",
    "build:lib": "rimraf dist && ng-packagr -p lib/package.json && gulp",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test packages",
    "tdd": "ng test dev --watch",
    "commit": "git-cz",
    "analyze": "npm run build:lib && cpr dist/ngx-timeago demo/ngx-timeago && npm run ng:build:demo  -- --stats-json && rimraf demo/ngx-timeago && webpack-bundle-analyzer dist/demo/stats.json"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.1",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.0",
    "@types/node": "~10.1.0",
    "bootstrap": "^4.1.1",
    "codelyzer": "~4.3.0",
    "commitizen": "2.10.1",
    "cpr": "^3.0.0",
    "cz-conventional-changelog": "2.1.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-typescript": "^4.0.2",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "merge2": "^1.2.2",
    "ng-packagr": "4.0.0-rc.0",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.1",
    "ts-node": "~6.0.0",
    "tsickle": "^0.29.0",
    "tslib": "^1.7.1",
    "tslint": "~5.9.0",
    "typescript": "~2.7.2",
    "webpack-bundle-analyzer": "^2.11.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
